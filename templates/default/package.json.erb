{
  "name":        "chef-managed-hubot",
  "version":     "<%= @version %>",
  "author":      "GitHub Inc.",
  "private":     <%= @private %>,
  "keywords":    "github hubot campfire bot",
  "description": "A simple helpful Robot for your Company",
  "licenses":     [{
    "type":       "MIT",
    "url":        "http://github.com/github/hubot/raw/master/LICENSE"
  }],

  "repository" : {
    "type": "git",
    "url":  "<%= @repository %>"
  },

  "dependencies": {
    "hubot": "git+<%= @repository %>",
    "hubot-scripts": "<%= @scripts_version %>",
    <%- @dependencies.each_pair do |dep, version| -%>
    "<%= dep %>": "<%= version %>",
    <%- end -%>
    "optparse": "1.0.3"
  },

  "engines": {
    "node": ">= 0.8.x",
    "npm": "1.1.x"
  }
}
